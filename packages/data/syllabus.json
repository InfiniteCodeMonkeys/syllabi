{
  "name": "Introduction to Computer Science",
  "description": "This course is an introduction to the fundamental concepts of computer science. Topics include: computer hardware, software, programming, algorithms, data structures, and the Internet.",
  "learningObjectives": [
    "Understand the basic concepts of programming, including variables, data types, and control structures",
    "Write simple programs in a high-level programming language",
    "Understand and implement basic algorithms and data structures, such as arrays, linked lists, and trees.",
    "Understand the basics of computer architecture, including the CPU, memory, and storage",
    "Develop critical thinking skills and problem-solving techniques"
  ],

  "weeks": [
    {
      "week": 1,
      "topics": ["Introduction to Computer Science"],
      "readings": [
        {
          "chapters": ["Chapter 1: Introduction to Computers and Programming"],
          "author": "John G. Kemeny and Thomas E. Kurtz",
          "book": "Dartmouth Basic",
          "link": "https://www.dartmouth.edu/basicfifty/basicstext/Chapter1.pdf"
        },
        {
          "chapters": [
            "Chapter 1: An Overview of Computers and Programming Languages"
          ],
          "author": "Richard Riehle",
          "book": "Object-Oriented Programming and Java",
          "link": "https://people.cs.aau.dk/~normark/oop-csharp/html/notes/intro-1-up.html"
        }
      ],
      "videos": [
        {
          "title": "What is Computer Science?",
          "author": "CrashCourse",
          "link": "https://www.youtube.com/watch?v=O5nskjZ_GoI"
        },
        {
          "title": "Programming Basics",
          "author": "Programming with Mosh",
          "link": "https://www.youtube.com/watch?v=bgwIj_neWzU"
        }
      ]
    },
    {
      "week": 2,
      "topics": ["Programming Fundamentals"],
      "readings": [
        {
          "chapters": ["Chapter 2: Basic Elements of Java"],
          "author": "Cay S. Horstmann",
          "book": "Big Java",
          "link": "https://www.amazon.com/dp/1119496474"
